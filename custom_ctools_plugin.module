include_once 'custom_ctools_plugin.features.inc';

/**
 * Implements hook_ctools_plugin_directory().
 */
function custom_ctools_plugin_ctools_plugin_directory($module, $plugin) {
  if ($module == 'ctools' && !empty($plugin)) {
    return "plugins/$plugin";
  }
}